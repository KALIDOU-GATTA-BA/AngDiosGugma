{% extends 'base.html.twig' %}

{% block title %}Ang Dios Gugma{% endblock %}
	
{% block body %}
<div class="container"><br><br>            
		<div class="row"> 
			{% for video in videos %}

                    <div class="col-md-12 col-xs-12 col-sm-12 col-lg-4">
                       <div class="news">   
	                       <h3 style="text-align:center;border-radius: 12px;border: 2px solid;font: italic  15px/30px Georgia, serif;">
	                             {{video.title}}                                                  
	                       </h3>                    
	                                <iframe src="{{video.link}}" width="100%" height="100%" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true" allowFullScreen="true"></iframe> <br> 
                       </div>
                    </div>
            {% endfor %}
        </div>          
</div>  
{% endblock %}